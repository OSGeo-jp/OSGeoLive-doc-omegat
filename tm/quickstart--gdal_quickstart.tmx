      <tuv xml:lang="en">
        <seg>GDAL Raster Tutorial</seg>
      </tuv>
      <tuv xml:lang="ja">
        <seg>GDAL ラスターのチュートリアル</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="en">
        <seg>https://trac.osgeo.org/gdal/wiki/UserDocs/RasterProcTutorial</seg>
      </tuv>
      <tuv xml:lang="ja">
        <seg>https://trac.osgeo.org/gdal/wiki/UserDocs/RasterProcTutorial</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="en">
        <seg>GDAL Workshop</seg>
      </tuv>
